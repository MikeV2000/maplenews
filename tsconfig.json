{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowJs": false,
    "resolveJsonModule": true,
    "strict": true,
    "esModuleInterop": true,
    "pretty": true,
    "declaration": false,
    "emitDeclarationOnly": true,
    "noImplicitAny": true,
    "importsNotUsedAsValues": "error",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true
  }
}